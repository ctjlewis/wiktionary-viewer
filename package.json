{
  "name": "wiktionary-portal",
  "platform": "browser",
  "version": "0.0.1",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "style": "dist/bundle.css",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./styles": "./dist/bundle.css",
    "./styles/*": "./dist/components/*/index.css",
    "./package.json": "./package.json",
    "./*": "./dist/components/*/index.js"
  },
  "scripts": {
    "export": "tsmodule build",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "test": "ava",
    "prepublishOnly": "yarn build && yarn test"
  },
  "ava": {
    "timout": 240000,
    "files": [
      "test/**/*.test.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--no-warnings",
      "--loader=@tsmodule/tsmodule"
    ]
  },
  "resolutions": {
    "@types/react": "^17.0.38"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@tsmodule/react": "^12.0.0",
    "@tsmodule/tsmodule": "^40.0.4",
    "@types/node": "^17.0.42",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "autoprefixer": "^10.4.7",
    "ava": "^4.3.0",
    "cssnano": "^5.1.11",
    "eslint": "^8.17.0",
    "eslint-config-next": "^12.1.6",
    "next": "^12.1.6",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "tailwindcss": "^3.1.2",
    "typescript": "^4.7.3"
  }
}
